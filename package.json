{
  "name": "vue-use-firebase",
  "version": "0.2.1",
  "private": false,
  "description": "Vue.js Composition API functions for Firebase",
  "author": "Mike Stokes (apparchitects.nz@gmail.com)",
  "scripts": {
    "build": "npx vue-cli-service build --target lib --name vue-use-firebase 'src/index.ts'",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "test": "vue-cli-service test:unit"
  },
  "files": [
    "dist/*",
    "src/*",
    "*.json",
    "*.js",
    "*.ts"
  ],
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@types/node": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@vue/cli-plugin-babel": "^4.2.0",
    "@vue/cli-plugin-eslint": "^4.2.0",
    "@vue/cli-plugin-typescript": "^4.2.2",
    "@vue/cli-plugin-unit-jest": "^4.2.2",
    "@vue/cli-service": "^4.2.0",
    "@vue/composition-api": "^0.3.4",
    "@vue/eslint-config-typescript": "^5.0.1",
    "@vue/test-utils": "1.0.0-beta.31",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "firebase": "^7.0.0",
    "firebase-admin": "^8.9.2",
    "firestore-jest-mock": "^0.2.1",
    "typescript": "^3.7.0",
    "vue": "^2.6.0",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {},
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "bugs": "https://github.com/mikestokes/vue-use-firebase/issues",
  "homepage": "https://github.com/mikestokes/vue-use-firebase",
  "keywords": [
    "vue",
    "composition-api",
    "vue-composition-api",
    "firebase",
    "firebase-firestore",
    "firebase-cloud-firestore",
    "vuejs",
    "typescript",
    "javascript",
    "vue-hooks"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikestokes/vue-use-firebase.git"
  }
}
